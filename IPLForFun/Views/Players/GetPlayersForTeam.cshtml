@model IEnumerable<IPLForFun.Models.Player>


<h4> @ViewBag.TeamName</h4>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        
        <td>
            @Html.ActionLink("Select", "Edit", new { id=item.PlayerId }))
        </td>
    </tr>
}

</table>
